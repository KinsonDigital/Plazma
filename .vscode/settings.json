{
    "cSpell.words": [
        "Kinson",
        "Plazma",
        "pwsh",
        "runsettings"
    ],
    "dotnet.defaultSolution": "Plazma.sln",
    "[csharp]": {
		"editor.insertSpaces": false,
		"editor.indentSize": 4,
    },
	"[jsonc]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
	},
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
	},
	"editor.detectIndentation": false,
	"[github-actions-workflow]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	}
}
